###onMousePress###
def onMousePress(mouseX, mouseY):
    if (Time1.value=='6:00'):
        Time1.value='12:00'

    else:
        Time1.value='6:00'
 
    Sun1.centerX+=220
    Sun2.centerX+=220
    
    if (Sun1.centerX==860):
        Sun1.centerX=-20
        Sun2.centerX=-20
        Sun1.centerY=75
        Sun2.centerY=75
        
    if (Sun1.centerX==640):
        Moon.visible=True
        
    else:
        Moon.visible=False
        
    if (Sun1.centerX>0):
        Time2.value='PM'
        
    if (Sun1.centerX==640):
        Time2.value='AM'
        
    if (Sun1.centerX==200):
        Sun1.centerY=40
        Sun2.centerY=40
        
    if (Sun1.centerX==420):
        Sun1.centerY=75
        Sun2.centerY=75
        
    
        
###If Statements and toFront/toBack###
    if (Time1.value=='6:00' and Time2.value=='AM'):
        Sky4.visible=False
        Sky1.visible=True
        Sky2.visible=False
        Sky3.visible=False
        
###People/Car Group 1        

        PG1.toFront()
        
        CG1.toFront()

###People/Car Group 4 

        PG4.toBack()
        
        CG4.toBack()
        
        Time2.value='AM'
        
        Sky1.toBack()
        Sky2.toBack()
        Sky3.toBack()
        Sky4.toBack()
        
    
    elif (Time1.value=='12:00' and Time2.value=='PM'):
        Sky3.visible=False
        Sky2.visible=True
        Sky1.visible=False
        Sky4.visible=False

###People/Car Group 1

        PG1.toBack()
        
        CG1.toBack()

###People/Car Group 2

        PG2.toFront()

        CG2.toFront()
        
        Time2.value='PM'
        
        Sky1.toBack()
        Sky2.toBack()
        Sky3.toBack()
        Sky4.toBack()

    elif (Time1.value=='6:00' and Time2.value=='PM'):
        Sky2.visible=False
        Sky3.visible=True
        Sky1.visible=False
        Sky4.visible=False
        
###People/Car Group 2

        PG2.toBack()
        
        CG2.toBack()

###People/Car Group 3

        PG3.toFront()
        
        CG3.toFront()
        
        Time2.value='PM'
        
        Sky1.toBack()
        Sky2.toBack()
        Sky3.toBack()
        Sky4.toBack()
    
    elif (Time1.value=='12:00' and Time2.value=='AM'):
        Sky3.visible=False
        Sky4.visible=True
        Sky2.visible=False
        Sky1.visible=False
        
###People/Car Group 3 

        PG3.toBack()
        
        CG3.toBack()
        
###People/Car Group 4 

        PG4.toFront()
        
        CG4.toFront()
        
        Time2.value='AM'
        
        Sky1.toBack()
        Sky2.toBack()
        Sky3.toBack()
        Sky4.toBack()

###People/Car Group 2

PG2.toBack()

CG2.toBack()

###People/Car Group 3

PG3.toBack()

CG3.toBack()

###People/Car Group 4

PG4.toBack()

CG4.toBack()


TimeBox.toFront()
Time1.toFront()
Time2.toFront()
Moon.toFront()
